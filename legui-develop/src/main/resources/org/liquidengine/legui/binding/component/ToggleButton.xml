<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.component.ToggleButton" to="toggleButton" default="true"
    inherit="org/liquidengine/legui/binding/component/Component.xml">
    <bind field="toggled"/>
    <bind field="togglededBackgroundIcon" to="togglededBackgroundIcon"/>
    <bind field="toggledBackgroundColor" to="toggledBackgroundColor">
        <using-binding path="org/liquidengine/legui/binding/common/Vector4fColor.xml"/>
    </bind>
</class-binding>