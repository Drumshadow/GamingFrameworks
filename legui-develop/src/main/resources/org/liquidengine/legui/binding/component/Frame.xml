<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.component.Frame" to="frame" default="true">
    <bind field="size" to="size" as="field">
        <using-binding path="org/liquidengine/legui/binding/common/Vector2fSize.xml"/>
        <using-accessor class="org.liquidengine.legui.binding.accessor.frame.FrameSizeAccessor"/>
    </bind>

    <bind field="componentLayer">
        <using-accessor class="org.liquidengine.legui.binding.accessor.frame.FrameComponentLayerAccessor"/>
    </bind>

    <bind field="layers" as="field">
        <using-accessor class="org.liquidengine.legui.binding.accessor.frame.FrameLayersAccessor"/>
    </bind>

</class-binding>