<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.component.SelectBox" to="selectBox" default="true"
    inherit="org/liquidengine/legui/binding/component/Component.xml">

    <unbind field="childComponents"/>

    <bind field="selectedElement"/>
    <bind field="elementHeight"/>
    <bind field="buttonWidth"/>
    <bind field="visibleCount"/>
    <bind field="expandIcon" to="expandIcon"/>
    <bind field="collapseIcon" to="collapseIcon"/>

    <bind field="elements" to="elements">
        <using-accessor class="org.liquidengine.legui.binding.accessor.selectbox.SelectBoxElementsAccessor"/>
    </bind>

</class-binding>