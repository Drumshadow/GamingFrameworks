<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.component.ScrollablePanel" to="scrollablePanel" default="true"
    inherit="org/liquidengine/legui/binding/component/Component.xml">

    <unbind field="childComponents"/>

    <bind field="verticalScrollBar" to="verticalScrollBar"/>
    <bind field="horizontalScrollBar" to="horizontalScrollBar"/>
    <bind field="container" to="container"/>

</class-binding>