<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.component.RadioButton" to="radioButton" default="true"
    inherit="org/liquidengine/legui/binding/component/Component.xml">
    <bind field="textState" to="textState"/>
    <bind field="iconUnchecked" to="iconUnchecked"/>
    <bind field="iconChecked" to="iconChecked"/>
    <bind field="checked"/>
    <bind field="radioButtonGroup">
        <using-converter class="org.liquidengine.legui.binding.converter.component.RadioButtonGroupConverter"/>
    </bind>
</class-binding>