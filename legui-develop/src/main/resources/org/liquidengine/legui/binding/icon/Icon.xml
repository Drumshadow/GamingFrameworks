<class-binding xmlns="binding.legui.liquidengine.org"
    for="org.liquidengine.legui.icon.Icon" to="icon">
    <bind field="size" as="field">
        <using-binding path="org/liquidengine/legui/binding/common/Vector2fSize.xml"/>
    </bind>
    <bind field="position" as="field">
        <using-binding path="org/liquidengine/legui/binding/common/Vector2f.xml"/>
    </bind>
    <bind field="horizontalAlign" to="hAlign"/>
    <bind field="verticalAlign" to="vAlign"/>
</class-binding>